{
    "ConnectionStrings": {
        "SqlConnection": "",
        "AzureStorageConnection": ""
    },
    "AppSettings": {
        "ApplicationName": "",
        "ApplicationDescription": "",
        "StorageFolder": "",
        "SupportedCultures": []
    },
    "EmailSettings": {
        "ApiKey": "",
        "EmailAddress": ""
    },
    "SwaggerSettings": {
        "Enabled": false,
        "UserName": "",
        "Password": ""
    },
    "Authentication": {
        "DefaultScheme": "",
        "JwtBearer": {
            "SchemeName": "",
            "SecurityKey": "",
            "Issuers": [],
            "Audiences": [],
            "ExpirationTime": "00:00:00",
            "ClockSkew": "00:00:00",
            "EnableJwtBearerService": false
        }
    },
    "Serilog": {
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "Microsoft.AspNetCore.Identity": "Error",
                "Microsoft.EntityFrameworkCore.Database.Command": "Warning",
                "System": "Warning"
            }
        },
        "WriteTo": [
            {
                "Name": "Debug"
            },
            {
                "Name": "Console",
                "Args": {
                    "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
                    "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj} <s:{SourceContext}>{NewLine}{Exception}"
                }
            }
        ],
        "Enrich": [ "FromLogContext" ]
    },
    "AllowedHosts": "*"
}